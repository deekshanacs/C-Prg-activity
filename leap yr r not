// to check whther the given year is leap year ornot//

#include<stdio.h>
int main()
{
    int yr;
    printf("Enter a year:");
    scanf("%d",&yr);
    if(yr%4==0)
    {
        printf("The given year %d is a leap year",yr);
    }
    else
    {
        printf("The given year %d is not a leap year",yr);
    }
    return 0;
}
