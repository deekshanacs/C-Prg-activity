//to find area of the triangle//

#include<stdio.h>
int main()
{
    int b,h,area;
    printf("Enter the base of triangle :");
    scanf("%d",&b);
    printf("\nEnter the height of triangle :");
    scanf("%d",&h);
    area=(b*h/2);
    printf("\nThe area of the triangle = %d",area);
    return 0;
}
