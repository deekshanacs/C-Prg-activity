/*Write a C program to accept a coordinate point in an XY 
coordinate system and determine in which 
quadrant the coordinate point lies*/

#include<stdio.h>
int main()
{
    int x,y;
    printf("Enter the x coordinate: ");
    scanf("%d",&x);
    printf("Entee the y coordinate:");
    scanf("%d",&y);
    if(x>0 && y>0)
    {
        printf("The points %d,%d lies in the Ist quadrant",x,y);
    }
    else if(x<0 && y>0)
    {
        printf("the points %d,%d lies in the IInd quadrant",x,y);
    }
    else if (x<0 && y<0)
    {
        printf("the points %d,%d lies in IIIrd quadrant",x,y);
    }
    else if(x==0 && y==0)
    {
        printf("The points %d,%d lies on origin",x,y);
    }
    else
    {
        printf("The points %d,%d lies in IVth quadrant",x,y);
    }
    return 0;
}
